# ============================================
# YieldPro - Environment Configuration
# ============================================
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control!

# ============================================
# Frontend Configuration
# ============================================
VITE_WALLETCONNECT_PROJECT_ID=''

# ============================================
# RPC URLs
# ============================================
# Get RPC URLs from Alchemy, Infura, or QuickNode
# Free public RPCs are provided as defaults but may be rate-limited

ETHEREUM_RPC_URL=https://eth.llamarpc.com
BASE_RPC_URL=https://mainnet.base.org
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
OPTIMISM_RPC_URL=https://mainnet.optimism.io

# ============================================
# Deployment Wallet
# ============================================
# Private key for deploying contracts (DO NOT share!)
# Needs ETH on each chain for gas

PRIVATE_KEY=0x...

# ============================================
# Vincent AI Configuration
# ============================================
# Vincent executor wallet (controls automation)
# Create a dedicated wallet for Vincent
# Needs ETH on each chain for rebalancing transactions

VINCENT_EXECUTOR_ADDRESS=0x...
VINCENT_PRIVATE_KEY=0x...

# ============================================
# Deployed Contract Addresses
# ============================================
# Fill these in after deployment
# Run deployment scripts and copy addresses here

# VincentAutomation (deployed once on Base)
VINCENT_AUTOMATION_ADDRESS=

# Vaults (one per chain)
VITE_VAULT_ADDRESS=0x0000000000000000000000000000000000000000
BASE_VAULT_ADDRESS=
ARBITRUM_VAULT_ADDRESS=
ETHEREUM_VAULT_ADDRESS=
OPTIMISM_VAULT_ADDRESS=

# ============================================
# Protocol Adapters
# ============================================
# Adapter addresses from deployment scripts

# Base Adapters
BASE_AAVE_ADAPTER=
BASE_MORPHO_ADAPTER=
BASE_COMPOUND_ADAPTER=

# Arbitrum Adapters
ARBITRUM_AAVE_ADAPTER=
ARBITRUM_MORPHO_ADAPTER=

# Ethereum Adapters
ETHEREUM_AAVE_ADAPTER=
ETHEREUM_COMPOUND_ADAPTER=
ETHEREUM_MORPHO_ADAPTER=

# Optimism Adapters
OPTIMISM_AAVE_ADAPTER=

# ============================================
# Cross-Chain Bridge
# ============================================
# Avail Nexus contract addresses

NEXUS_CONTRACT_ADDRESS=

# ============================================
# Testing & Development
# ============================================
# Test user wallet for demo

TEST_USER_ADDRESS=0x...
TEST_USER_PRIVATE_KEY=0x...

# ============================================
# API Keys (for contract verification)
# ============================================
# Get from respective block explorers

ETHERSCAN_API_KEY=
BASESCAN_API_KEY=
ARBISCAN_API_KEY=
OPTIMISTIC_ETHERSCAN_API_KEY=

# ============================================
# Notifications (Optional)
# ============================================
# For production monitoring and alerts

SLACK_WEBHOOK_URL=
ADMIN_EMAIL=
TEAM_EMAIL=
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# ============================================
# Deployment Configuration
# ============================================
# Deployment preferences

# Deploy to testnet first? (true/false)
USE_TESTNET=false

# Testnet RPC URLs (if using testnets)
BASE_SEPOLIA_RPC_URL=
ARBITRUM_SEPOLIA_RPC_URL=

# ============================================
# Notes
# ============================================
# 1. Never commit .env to git (it's in .gitignore)
# 2. Keep private keys secure (use hardware wallet in production)
# 3. Fund deployer wallet with ~0.1 ETH per chain
# 4. Fund Vincent executor wallet with ~0.1 ETH per chain
# 5. Get RPC URLs from:
#    - Alchemy: https://alchemy.com
#    - Infura: https://infura.io
#    - QuickNode: https://quicknode.com
